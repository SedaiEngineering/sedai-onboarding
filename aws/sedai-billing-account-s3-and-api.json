{
	"Version": "2012-10-17T00:00:00.000Z",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": ["s3:Get*", "s3:List*"],
			"Resource": [
				{
					"Fn::Sub": "arn:${AWS::Partition}:s3:::sedai-cur-bucket-${AWS::AccountId}"
				},
				{
					"Fn::Sub": "arn:${AWS::Partition}:s3:::sedai-cur-bucket-${AWS::AccountId}/*"
				}
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"pricing:DescribeServices",
				"pricing:GetAttributeValues",
				"pricing:GetProducts",
				"pricing:GetPriceListFileUrl",
				"pricing:ListPriceLists"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": ["cur:DescribeReportDefinitions", "cur:GetUsage*"],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": ["ce:GetCost*", "ce:DescribeCost*", "ce:List*", "ce:GetReservation*", "ce:GetSavings*"],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": ["purchase-orders:Get*", "purchase-orders:List*"],
			"Resource": "*"
		},
		{
			"Effect": "Deny",
			"Action": [
				"s3:GetObject",
				"s3:GetObjectVersion",
				"s3:GetObjectAcl",
				"s3:GetObjectTagging",
				"ec2:DescribeVpnConnections",
				"ec2:DescribeClientVpn*",
				"ec2:DescribeAddressTransfers",
				"ec2:DescribeCarrierGateways",
				"ec2:DescribeClientVpn*",
				"ec2:DescribeCoipPools",
				"ec2:DescribeCustomerGateways",
				"ec2:DescribeFlowLogs",
				"ec2:DescribeIpamByoasn",
				"ec2:DescribeIpam*",
				"ec2:DescribeLocalGateway*",
				"ec2:DescribeManagedPrefixLists",
				"ec2:DescribeTrafficMirror*",
				"ec2:DescribeTransitGateway*",
				"ec2:DescribeTrunkInterfaceAssociations",
				"ec2:DescribeVerifiedAccess*",
				"ec2:DescribeVpcEndpoint*",
				"ec2:DescribeVpcPeeringConnections",
				"ec2:DescribeVpnGateways"
			],
			"Resource": "*"
		}
	]
}
