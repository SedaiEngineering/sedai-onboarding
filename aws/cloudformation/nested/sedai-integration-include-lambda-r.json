{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "lambdareadlambda",
			"Effect": "Allow",
			"Action": ["lambda:Get*", "lambda:List*"],
			"Resource": "*"
		},
		{
			"Sid": "lambdareadapplicationautoscaling",
			"Effect": "Allow",
			"Action": ["application-autoscaling:Describe*"],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": ["dynamodb:List*", "dynamodb:Describe*"],
			"Resource": "*"
		}
	]
}
